           <% if @cordinate.liked_users && @likecordinates !=nil %>
                          <p><i class="far fa-thumbs-up fa-1x"></i>いいね:合計<%= @likecordinates_count %></p>
                          <% unless @cordinate.liked_users.nil? %>
                            <% @cordinate.liked_users.each do |user| %>
                            <div class= "cordinate_responses">
                              <div class= "cordinate_responses">                             
                              <%= image_tag @user.picture.url, size: 150 unless @user.picture.blank? %>
                              <%= @user.name %>へ</li>
                             <br>
                             <div class = "comment_box">
                              <li><%= user.name %>さんがいいねしました。</li>
                              </div>
                              <br>
                                <% if user.id === current_user.id %>
                              <li><%= button_to "いいねを取り消す" , likecordinate_delete_path( user_id: user.id, likecordinate_id: user.likecordinates, id: @cordinate.id ),
                                :controller=>"likecordinates", :action=> "destroy", :method => "delete", class: "comment_like_btn" %></li>
                                <% end %>
                                </div>
                              <% end %>
                                <% end %>
                                  <% end %>
